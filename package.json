{
  "name": "skill-net",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/styles/smui.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile static/styles/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ext .js,.ts,.svelte src",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.0.96",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui/fab": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/layout-grid": "^6.1.0",
    "@smui/linear-progress": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/segmented-button": "^6.0.0-beta.16",
    "@smui/select": "^6.0.0-beta.16",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "date-fns": "^2.29.1",
    "date-picker-svelte": "^2.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-svelte": "^2.8.0",
    "prettier": "^2.7.0",
    "prettier-plugin-svelte": "^2.7.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.49.0",
    "svelte-check": "^2.6.0",
    "svelte-eslint-parser": "^0.18.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-scrollto-element": "^0.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^3.1.0-beta.1",
    "vitest-github-actions-reporter": "^0.8.1"
  },
  "type": "module",
  "dependencies": {
    "@typescript-eslint/parser": "^5.37.0",
    "firebase": "^9.8.4",
    "svelte-typed-js": "^1.0.3"
  }
}